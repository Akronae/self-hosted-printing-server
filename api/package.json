{
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@octokit/rest": "^21.0.2",
    "@prisma/client": "6",
    "@sentry/cli": "^2.36.5",
    "@sentry/profiling-node": "^8.32.0",
    "canvas": "^3.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "ipp": "^2.0.1",
    "pdf-to-img": "^4.4.0",
    "png-to-jpeg": "^1.0.1",
    "prisma": "6",
    "prisma-class-generator": "^0.2.11",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.16",
    "@types/express": "^4.17.17",
    "@types/ipp": "^2.0.5",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.9",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.1.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "license": "MIT",
  "name": "api",
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610",
  "private": true,
  "scripts": {
    "build": "yarn db:gen && nest build",
    "db:format": "yarn env -- yarn prisma format",
    "db:gen": "yarn env -- yarn prisma generate",
    "db:migrate:dev": "yarn env -- yarn prisma migrate dev --name init",
    "db:reset": "rm -rf prisma/dev.db uploads && yarn env -- yarn prisma db push && yarn db:gen",
    "db:studio": "yarn env -- yarn prisma studio",
    "dev": "nest start --watch --type-check",
    "env": "yarn tsx src/scripts/env.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "nest:gen": "yarn tsx src/scripts/gen/nest.ts",
    "start": "nest start",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json",
    "test:watch": "jest --watch",
    "v:major": "npm version major -m \"chore: bump major to %s\"",
    "v:minor": "npm version minor -m \"chore: bump minor to %s\"",
    "v:patch": "npm version patch -m \"chore: bump patch to %s\""
  },
  "version": "0.0.2"
}
